{
  "name": "jochemgruter/laravel-resources",
  "description": "Laravel CRUD package",
  "keywords": [
    "laravel", "models", "resources"
  ],
  "license": "MIT",
  "authors":[
    {
      "name": "Jochem Gruter",
      "email": "jochemgruter@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "5.6|6.0.*"
  },
  "require-dev": {
    "orchestra/database": "3.7.* | 3.8.*",
    "orchestra/testbench": "3.7.* | 3.8.*",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Gruter\\ResourceViewer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gruter\\ResourceViewer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Resource": "Gruter\\ResourceViewer\\Facades\\Resource"
      }
    }
  }
}